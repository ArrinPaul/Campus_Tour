{
  "version": 2,
  "buildCommand": "cd campus-360-app && npm install && npm run build",
  "outputDirectory": "campus-360-app/dist",
  "framework": "vite",
  "installCommand": "cd campus-360-app && npm install",
  "devCommand": "cd campus-360-app && npm run dev",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
